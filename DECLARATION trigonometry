#include<bits/stdc++.h>
using namespace std;
const double pi = 2*acos(0);
#define cos(a) cos(a*pi/180)
#define sin(a) sin(a*pi/180)
#define tan(a) tan(a*pi/180)
#define cosi(a) acos(a)/(pi/180)
#define sini(a) asin(a)/(pi/180)
#define tani(a) atan(a)/(pi/180)

int main()
{
   double x = tan(25);
   
   cout<<x<<endl;
}
